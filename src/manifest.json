{
  "name": "PrettyCode",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "First extension",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [ {
    "js": [
      "vendor/jquery/dist/jquery.min.js",
      "vendor/highlightjs/highlight.pack.js",
      "content_script.js"],
    "css": [
      "styles/main.min.css"
    ],
    "matches": [
      "http://*.facebook.com/*",
      "https://*.facebook.com/*",
      "http://facebook.com/*",
      "https://facebook.com/*"
    ]
  } ],
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions" : [
    "tabs",
    "activeTab",
    "storage",
    "http://*.facebook.com/*",
    "https://*.facebook.com/*",
    "http://facebook.com/*",
    "https://facebook.com/*"
  ]
}
